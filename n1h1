if r == 'y':
  print '-'*30
  print "contagiado"
if (r == 'y'):
    print "Viste al DR?:"
    r2 = raw_input('y/n: ')
    if (r2 == 'n'):
      print "Te recomiendo que vayas a visitarlo"

    
    
  

            
            














